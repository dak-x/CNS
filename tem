

struct Details{
    int Name
    Point P
    desc: String
    scores : list(int)
    flag: Option(String)
}

builder pattern

    Details_Builder d = new();
    Details main = d.with_name(Name)
                    .with_Point(Pt)
                    .with_desc(Str)
                    .add_score(10).add_score(20)
                    .build()

